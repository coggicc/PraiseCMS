@using System.Configuration

@{
    ViewBag.Title = "Index";
}

<style>
    .feature-list {
        font-size: 18px;
        line-height: 2;
    }

        .feature-list i {
            color: #3699FF;
        }
</style>
<header class="page header color-1 overlay alpha-8 image-background cover overlay gradient gradient-43" style="background-image: url('../../Content/img/bg/waves.jpg')">
    <div class="divider-shape">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1000 100" preserveAspectRatio="none" class="shape-waves" style="left: 0; transform: rotate3d(0,1,0,180deg);">
            <path class="shape-fill shape-fill-1" d="M790.5,93.1c-59.3-5.3-116.8-18-192.6-50c-29.6-12.7-76.9-31-100.5-35.9c-23.6-4.9-52.6-7.8-75.5-5.3c-10.2,1.1-22.6,1.4-50.1,7.4c-27.2,6.3-58.2,16.6-79.4,24.7c-41.3,15.9-94.9,21.9-134,22.6C72,58.2,0,25.8,0,25.8V100h1000V65.3c0,0-51.5,19.4-106.2,25.7C839.5,97,814.1,95.2,790.5,93.1z" />
        </svg>
    </div>
    <div class="container" style="">
        <div class="row">
            <div class="col-md-12">
                <h1>Free giving tools</h1>
                <p class="lead color-1">Try Praise Premium risk free for @ConfigurationManager.AppSettings["FreeTrialDays"] days. No contracts. Cancel anytime.</p>
            </div>
        </div>
    </div>
</header>
<section class="">
    <div class="container">
        <div class="row">
            <div class="col-md-6 mt-5">
                <div class="card text-center">
                    <div class="pricing card-header p-5 bg-light d-flex align-items-center flex-column">
                        <h4 class="bold">Basic</h4>
                        <div style="font-family: Poppins, sans-serif;font-size: 3.25rem;">FREE</div>
                        <p>Elevate your giving experience with our digital tools, including streamlined donations, fund allocation, and powerful givign reports.</p>
                    </div>
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item">Digital Giving (Debit/Credit Cards &amp; ACH)</li>
                        <li class="list-group-item">Giving Funds</li>
                        <li class="list-group-item">Giving Reports</li>
                    </ul>
                    <div class="card-body"><a href="@($"{ConfigurationManager.AppSettings["DashboardUrl"]}Account/Register")" class="btn btn-rounded btn-outline-primary py-3 px-5">Sign up Free</a></div>
                </div>
            </div>
            <div class="col-md-6 mt-5">
                <div class="card text-center">
                    <div class="pricing card-header p-5 bg-primary color-1 d-flex align-items-center flex-column">
                        <h4 class="bold text-contrast">Premium</h4>
                        <div class="pricing-value"><span class="price text-contrast">39</span></div>
                        <p>Enhance your church operations with our digital tools, encompassing digital and offline giving options, comprehensive reporting, unlimited giving fund management, church family administration, and priority support.</p>
                    </div>
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item">Digital Giving (Debit/Credit Cards &amp; ACH)</li>
                        <li class="list-group-item">Offline Giving (Cash &amp; Checks)</li>
                        <li class="list-group-item">Robust Reporting &amp; Analytics</li>
                        <li class="list-group-item">Unlimited Giving Funds</li>
                        <li class="list-group-item">Church Family Management</li>
                        <li class="list-group-item">Priority Support</li>
                    </ul>
                    <div class="card-body"><a href="@($"{ConfigurationManager.AppSettings["DashboardUrl"]}Account/Register")" class="btn btn-rounded btn-accent py-3 px-5">Start Free Trial</a></div>
                </div>
            </div>
        </div>
    </div>
</section><!-- ./Plans features -->
